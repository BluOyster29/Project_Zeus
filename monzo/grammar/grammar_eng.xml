<?xml version="1.0" encoding="utf-8"?>
<grammar>

  <question speaker="user" predicate="account_details" type="wh_question">
    <one-of>
      <item>I want to check my account details</item>
      <item>What are my account details</item>
    </one-of>
  </question>

  <question speaker="system" predicate="account_details" type="wh_question">
    Your account details are as following <sort predicate="account_details"/>
  </question>

  <individual name="current">
    <item>current</item>
  </individual>


</grammar>
